<!DOCTYPE html>
<html>
<head>
  <title>Seatyourself</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

  <nav>
    <ul>
      <li>
        <%= link_to "Home", root_path %>
      </li>

      <li>
        <%= link_to "Sign Up", new_user_path %>
      </li>

      <li>
        <%= link_to "Log in", new_session_path%>
      </li>

      <li>
        <%= link_to 'Log out', session_path(user.id), method: :delete, data:{confirm: "You are about to lose your logged in privaliges"} %>
      </li>

    </ul>

  </nav>

<%= yield %>

</body>
</html>
